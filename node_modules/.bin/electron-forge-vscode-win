#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/cmd" ]; then
  exec "$basedir/cmd"  "$basedir/../@electron-forge/cli/script/vscode.cmd" "$@"
else
  exec cmd  "$basedir/../@electron-forge/cli/script/vscode.cmd" "$@"
fi
